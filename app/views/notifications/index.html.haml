%ul
  - @group_days.each do |day, notes|
    %h3
      %ul
        %li
          = day
        - notes.each do |note|
          %li
            = link_to "#{note.person.name.titleize}", person_path(note.person)
            = object_link(note)
            %span.description=" #{t('ago', :time => time_ago_in_words(note.created_at))}"
