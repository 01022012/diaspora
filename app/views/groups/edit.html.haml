- content_for :head do 
  = javascript_include_tag 'jquery-ui-1.8.4.custom.min.js'
  = javascript_include_tag 'group-edit.js'


%h1.big_text
  .back
    = link_to "⇧ #{@group.name}", @group

  = "Editing Groups"


%ul
- for group in @groups
  %li.group
    = group.name
    %ul
      -for person in group.people
        %li..person
          = person.real_name




#content_bottom
  .back
    = link_to "⇧ #{@group.name}", @group
