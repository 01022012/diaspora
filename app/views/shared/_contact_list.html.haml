.contact_list
  = search_field_tag :contact_search, "", :class => 'contact_list_search', :results => 5, :placeholder => t('.all_contacts')
  %ul
    - for contact in contacts
      %li
        %span.name
          = contact_link contact
        .right
          = aspect_membership_button(aspect_id, contact)

